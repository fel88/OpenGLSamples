<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net8.0-windows7.0</TargetFramework>
    <OutputType>WinExe</OutputType>
    <RootNamespace>MultipleLights</RootNamespace>
    <AssemblyName>MultipleLights</AssemblyName>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <UseWindowsForms>true</UseWindowsForms>
    <ImportWindowsDesktopTargets>true</ImportWindowsDesktopTargets>
  </PropertyGroup>
  <ItemGroup>
    <EmbeddedResource Include="shaders\3.1.light_cube.fs" />
    <EmbeddedResource Include="shaders\3.1.light_cube.vs" />
    <EmbeddedResource Include="shaders\3.1.materials.fs" />
    <EmbeddedResource Include="shaders\3.1.materials.vs" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.CSharp" Version="4.7.0" />
    <PackageReference Include="OpenTK">
      <Version>4.9.4</Version>
    </PackageReference>
    <PackageReference Include="OpenTK.GLControl">
      <Version>4.0.2</Version>
    </PackageReference>
    <PackageReference Include="System.Data.DataSetExtensions" Version="4.5.0" />
  </ItemGroup>
</Project>